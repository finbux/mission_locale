{% extends "base.html.twig" %}
{% block header %}
    <div id="container_logo">
        <span class="helper"></span>
        <a href="{{ path('main_homepage') }}"><img src="{{ asset('bundles/main/img/logo.png') }}" alt=""></a>
    </div>
{% endblock %}
{% block body %}

    {% block arianne %}
        {{ parent() }}<a href="{{ path('moins26_page') }}"> -> moins de 26 ans</a>
    {% endblock %}

{% endblock %}
{% block javascripts %}
    <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false&language=fr"></script>
    <script type="text/javascript" src="{{ asset('bundles/main/js/jquery.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/main/js/functions.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/main/js/coin-slider.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/main/js/jquery-ui-1.8.12.custom.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/main/js/display-description.js') }}"></script>
    <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script src="{{ path('fos_js_routing_js', {'callback': 'fos.Router.setData'}) }}"></script>
    <script type="text/javascript">
        $(document).ready(function() {
            $('#coin-slider').coinslider();
        });

    </script>

{% endblock %}



