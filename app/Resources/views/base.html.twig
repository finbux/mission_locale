<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}MLIFE{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('bundles/main/css/style.css') }}"
              type="text/css" media="all" />
        <link rel="stylesheet" href="{{ asset('bundles/main/css/coin-slider-styles.css') }}"
              type="text/css" media="all" />
        <link rel="stylesheet" href="{{ asset('bundles/main/css/outdatedbrowser.css') }}"/>
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>
<body>
{% block header %}
    <div id="header">
        <div id="container_logo">
            <span class="helper"></span>
            <img src="{{ asset('bundles/main/img/logo.png') }}" alt="">
        </div>
        <a href="https://www.facebook.com/MLIFE.GA?fref=ts"><div id="facebook"><img src="{{ asset('bundles/main/img/logo_facebook.png') }}" alt=""/>Suivre la MILFE</div></a>
        <div id="bulles">
            <a href="{{ path('moins26_page') }}"><img src="{{ asset('bundles/main/img/moins26.png') }}" alt=""/></a>
            <a href="{{ path('employeur_page') }}"><img src="{{ asset('bundles/main/img/employeur.png') }}" alt=""></a>
            <img src="{{ asset('bundles/main/img/plus26.png') }}" alt="">
            <div class="cb"></div>
        </div>

        <img id="silhouette" src="{{asset('bundles/main/img/silhouettes2.png') }}" />

    </div>
{% endblock %}



{% block body %}
    {% block arianne %}
        <a href="{{ path('main_homepage') }}">Accueil</a>
    {% endblock %}
{% endblock %}
{% block javascripts %}
    <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false&language=fr"></script>
    <script type="text/javascript" src="{{ asset('bundles/main/js/outdatedbrowser.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/main/js/jquery.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/main/js/functions.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/main/js/coin-slider.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/main/js/jquery-ui-1.8.12.custom.min.js') }}"></script>


    <script type="text/javascript">
        $(document).ready(function() {
            $('#coin-slider').coinslider();

            outdatedBrowser({
                bgColor: '#f25648',
                color: '#ffffff',
                lowerThan: 'transform',
                languagePath: ''
            });
        });
    </script>

{% endblock %}
</body>
</html>