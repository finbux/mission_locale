{% extends "FOSUserBundle::layout_admin.html.twig" %}
{% block title_admin %}Ajouter un sous item {% endblock %}
{% block content %}
    {% for type, messages in app.session.flashbag.all() %}
        {% for message in messages %}
            <div class="flash-{{ type }}">
                {{ message }}
            </div>
        {% endfor %}
    {% endfor %}
    <a href="{{ path('admin_sous_item', { 'id': app.request.get('id')} ) }}">
        <button>Page précédente</button>
    <a>
    <div class="update_data">
        {{ form_start(form) }}
        {{ form_row(form.nomSousItem) }}
        {{ form_row(form.description) }}
    </div>
    {{ form(form) }}
    {{ form_end(form) }}

{% endblock %}