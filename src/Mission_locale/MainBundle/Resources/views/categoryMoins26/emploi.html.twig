{% extends "::base.html.twig" %}
{% block title %}
    {{ parent() }} | je cherche un emploi
{% endblock %}
{% block body %}

    <div id="wrapper-categorie">
        <div class="arianne">
            {% block arianne %}
                <p>{{ parent() }} > Je cherche un emploi</p>
            {% endblock %}
        </div>
        <div class="cb"></div>
            <div class="block_emploi_auto">
                <h1>Je cherche un emploi</h1>
                <img src="{{ asset('bundles/main/img/fleche_emploi.png') }}" />
                <br/>
                    <span class="conseiller">Je rencontre un conseiller pour</span>
                    {% for sous_item in list_emploi %}
                        <p class="item">
                                {{ sous_item.nomSousItem }}
                        </p>
                        <input type="hidden" class="id_sous_item" value="{{ sous_item.id }}"/>
                        <hr/>
                    {% endfor %}
            </div>
        <div class="item_right border-emploi">

                <p class="description">

                </p>

                <img class="bandeau_sous_item" src="{{ asset('bundles/main/img/silhouette.png') }}" alt=""/>
        </div>
    </div>
    {% include "::footer.html.twig" %}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('bundles/main/js/loadSousItem.js') }}"></script>
{% endblock %}